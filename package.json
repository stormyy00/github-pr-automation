{
  "name": "pr-automation",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "flask": "cd api && pip3 install -r requirements.txt && python3 -m flask --app api/index run -p 5000 --reload",
    "next": "next dev",
    "dev": "concurrently \"next dev\" \"npm run flask\"",
    "build": "next build",
    "start": "next start",
    "check": "npx prettier --check .",
    "format": "npx prettier --write .",
    "eslint": "next lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
